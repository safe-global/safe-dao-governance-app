{
  "name": "safe-dao-governance-app",
  "homepage": "https://github.com/safe-global/safe-dao-governance-app",
  "license": "GPL-3.0",
  "version": "1.2.1",
  "scripts": {
    "build": "next build && next export",
    "lint": "tsc && next lint",
    "lint:fix": "next lint --fix",
    "generate-types:safe-token": "typechain --target ethers-v5 --out-dir ./src/types/contracts/safe-token './artifacts/safe-token/contracts/**/*[!.dbg].json'",
    "generate-types:delegate-registry": "typechain --target ethers-v5 --out-dir ./src/types/contracts/delegate-registry './artifacts/delegate-registry/contracts/**/*[!.dbg].json'",
    "generate-types": "yarn generate-types:safe-token && yarn generate-types:delegate-registry",
    "prettier": "prettier -w \"{src,pages,scripts}/**/*.{ts,tsx,css,js}\"",
    "postinstall": "hardhat compile && yarn run generate-types",
    "routes": "node scripts/generate-routes.js > src/config/routes.ts && prettier -w src/config/routes.ts",
    "start": "next dev",
    "test": "jest",
    "test:ci": "yarn test --ci --coverage --json --watchAll=false --testLocationInResults --outputFile=jest.results.json"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@gnosis.pm/safe-apps-provider": "^0.15.1",
    "@gnosis.pm/safe-apps-react-sdk": "^4.6.2",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.5",
    "@safe-global/safe-gateway-typescript-sdk": "^3.5.6",
    "@web3-onboard/coinbase": "^2.2.4",
    "@web3-onboard/core": "2.20.4",
    "@web3-onboard/injected-wallets": "^2.10.0",
    "@web3-onboard/keystone": "^2.3.7",
    "@web3-onboard/ledger": "2.3.2",
    "@web3-onboard/taho": "^2.0.5",
    "@web3-onboard/trezor": "^2.4.2",
    "@web3-onboard/walletconnect": "2.4.0",
    "bezier-easing": "^2.1.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^5.7.2",
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "delegate-registry": "gnosis/delegate-registry",
    "eslint": "^8.32.0",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "hardhat": "^2.12.6",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.3",
    "safe-token": "safe-global/safe-token",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4"
  }
}
